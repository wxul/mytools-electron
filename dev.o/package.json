{
  "name": "mytools-electron",
  "version": "0.9.0",
  "description": "Some Tools By Albert",
  "main": "main.js",
  "scripts": {
    "installar": "npm install --registry=https://registry.npm.taobao.org",
    "ele-install": "node ./node_modules/electron/install",
    "devstart": "electron dev.o/. -d",
    "devbuild": "webpack --config ./webpack.config.dev.js --watch",
    "build": "webpack --config ./webpack.config.build.js",
    "start": "electron .",
    "pack-win": "electron-packager . coooooool --platform=win32 --arch=ia32 --overwrite --out=out --prune=true --version-string.CompanyName=Albert --version-string.ProductName=Tools --download.mirror=https://npm.taobao.org/mirrors/electron/",
    "pack-mac": "electron-packager . coooooool --platform=darwin --arch=x64 --overwrite --out=out --prune=true --download.mirror=https://npm.taobao.org/mirrors/electron/",
    "pack-linux": "electron-packager . coooooool --platform=linux --arch=x64 --overwrite --out=out --prune=true --download.mirror=https://npm.taobao.org/mirrors/electron/",
    "package": "npm run pack-mac && npm run pack-win && npm run pack-linux",
    "win-install": "node ./scripts/wininstaller.js",
    "dmg-install": "node ./scripts/dmg.js"
  },
  "author": "albert",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-runtime": "^6.20.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "electron": "^1.4.13",
    "electron-installer-dmg": "^0.2.0",
    "electron-winstaller": "^2.5.1",
    "element-ui": "^1.1.3",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.9.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "marked": "^0.3.6",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "uglify-js": "^2.7.5",
    "url-loader": "^0.5.7",
    "vue": "^2.1.8",
    "vue-color": "^2.0.3",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^10.0.2",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.1.8",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "glob": "^7.1.0",
    "request": "^2.79.0",
    "rimraf": "^2.5.4",
    "uglify-js": "^2.7.5"
  }
}
